function FrontPage_Form1_Validator(theForm)
{

  if (theForm.Password.value == "")
  {
    alert("Please enter a value for the \"Password\" field.");
    theForm.Password.focus();
    return (false);
  }

  if (theForm.Password.value.length < 1)
  {
    alert("Please enter at least 1 characters in the \"Password\" field.");
    theForm.Password.focus();
    return (false);
  }

  if (theForm.Password.value.length > 10)
  {
    alert("Please enter at most 10 characters in the \"Password\" field.");
    theForm.Password.focus();
    return (false);
  }

  if (theForm.Valid_Email.value == "")
  {
    alert("Please enter a value for the \"Current Email Address\" field.");
    theForm.Valid_Email.focus();
    return (false);
  }

  if (theForm.Valid_Email.value.length < 1)
  {
    alert("Please enter at least 1 characters in the \"Current Email Address\" field.");
    theForm.Valid_Email.focus();
    return (false);
  }

  if (theForm.Valid_Email.value.length > 50)
  {
    alert("Please enter at most 50 characters in the \"Current Email Address\" field.");
    theForm.Valid_Email.focus();
    return (false);
  }
  return (true);
}
