function select(bool) {

	var frm = document.frmMain;
	var lCnt;
    if (frm.bClass.selectedIndex == 0) {
        frm.sClass.length = 1;
        frm.sClass.selectedIndex = 0;
    } else {
        if (bool) {
            frm.sClass.length = 1;
            frm.sClass.selectedIndex = 0;
