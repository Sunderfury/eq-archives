	var bClass = new Array("Win98", "Win98SE", "WinNT", "WinXP","Unix","Linux","その他");
	var sClass = new Array();
	sClass[0]  = new Array("VB", "VC", "Java","HTML","ASP","ACCESS");
	sClass[1]  = new Array("VB", "VC", "Java","HTML","ASP","ACCESS");
	sClass[2]  = new Array("VB", "VC", "Java","HTML","ASP","ACCESS");
	sClass[3]  = new Array("VB", "VC", "Java","HTML","ASP","ACCESS");
	sClass[4]  = new Array("Kilix", "GC", "pop3");
	sClass[5]  = new Array("Kilix", "GC", "pop3");
	sClass[6]  = new Array("IIS", "FireWorks", "DreamWeaver","Flash");

function select(bool) {

	var frm = document.frmMain;
	var lCnt;
    if (frm.bClass.selectedIndex == 0) {
        frm.sClass.length = 1;
        frm.sClass.selectedIndex = 0;
    } else {
        if (bool) {
            frm.sClass.length = 1;
            frm.sClass.selectedIndex = 0;
        }
        var val = sClass[frm.bClass.selectedIndex - 1];
        frm.sClass.length = val.length + 1;
        for (var lCnt = 0; lCnt < val.length; lCnt++) {
            frm.sClass.options[lCnt + 1].value = lCnt;
            frm.sClass.options[lCnt + 1].text = val[lCnt];
        }
    }
}

function window_onload() {
	document.frmMain.title.focus();
	//select(false);
}

function submit_form(flg) {
	var frm = document.frmMain;
	frm.delv.value = flg;
	frm.submit();
}

