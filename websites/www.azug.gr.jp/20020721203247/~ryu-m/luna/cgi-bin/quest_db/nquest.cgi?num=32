				<tr><td><span style="color:#333366;font-size:10pt">・Zone</span></td>
				<td><span style="color:#333366;font-size:10pt"><B>Connla</B></span></td></tr>
				<tr><td><span style="color:#333366;font-size:10pt">・Reward&nbsp;&nbsp;</span></td>
			<td><span style="color:#333366;font-size:10pt">Sword of Keeper</span></td></tr>
				<tr><td><span style="color:#333366;font-size:10pt">・Req</span></td>
			<td><span style="color:#333366;font-size:10pt">Blade Class</span></td></tr>
