	var noteswin = window;
	
	function note_frame() {
		if (!noteswin.closed)
			noteswin.focus();
		noteswin=window.open("/users/notes.asp?action=frame", "notes", "width=460,height=240,scrollbars=yes,resizable=yes");
	}
	function friends()    {
		if (!noteswin.closed)
			noteswin.focus();
		noteswin=window.open("/users/notes.asp?action=frame&tab=4", "notes", "width=460,height=240,scrollbars=yes,resizable=yes");
	}
