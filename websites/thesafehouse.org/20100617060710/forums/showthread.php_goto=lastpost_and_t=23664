			</div>
	<!-- / status icon and date -->
	<!-- / icon and title -->
    </td>
        <td align="right" valign="middle">
	<!-- controls -->
		
		
		
			<a href="newreply.php?do=newreply&amp;p=346281"><img src="images/buttons/quote.gif" alt="Reply With Quote" border="0" align="absmiddle" /></a>		
		
		
		
		
		
		
			
		
        

 

 
 
