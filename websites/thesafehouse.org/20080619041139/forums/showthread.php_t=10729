	<a href="report.php?p=133241"><img src="images/buttons/report.gif" alt="Report Bad Post" border="0" align="absmiddle" class="inlineimg" /></a>
	 &nbsp;
		<span class="pcount">#</span><a href="showpost.php?p=133241&amp;postcount=1" target="new" rel="nofollow" id="postcount133241" name="1" class="pcount"><strong>1</strong></a>
		
