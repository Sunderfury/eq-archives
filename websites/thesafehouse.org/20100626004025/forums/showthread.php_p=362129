      <tr class="">
        <td valign="middle">
        <!-- icon and title -->
			<div align="left" class="posttitle">
			<span class="posttitle">Re: Rogue Class Breakdown: Analysis &amp; Feedback</strong>
			</div>
	<!-- status icon and date -->
			<div align="left" >
			<a class="posttime" name="post362123">
			<span class="posttime">&nbsp;Posted:&nbsp;<strong>01-15-07, 12:29 PM</strong></span>
			
			</div>
	<!-- / status icon and date -->
	<!-- / icon and title -->
    </td>
