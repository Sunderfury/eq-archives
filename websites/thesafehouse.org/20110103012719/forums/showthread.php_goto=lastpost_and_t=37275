      <tr class="">
        <td valign="middle">
        <!-- icon and title -->
			<div align="left" class="posttitle">
			<span class="posttitle">Re: Building a PC from scratch</strong>
			</div>
	<!-- status icon and date -->
			<div align="left" >
			<a class="posttime" name="post502836">
			<span class="posttime">&nbsp;Posted:&nbsp;<strong>08-03-10, 09:47 PM</strong></span>
			
			</div>
	<!-- / status icon and date -->
	<!-- / icon and title -->
    </td>
