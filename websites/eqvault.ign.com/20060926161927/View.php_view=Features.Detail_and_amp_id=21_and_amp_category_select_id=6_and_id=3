function post_new_comment_OnSubmit(thisForm) {
if (typeof(popupWin) != 'undefined') popupWin.close();
filesChanged = false;
var comment = document.forms['post_new_comment'].post_new_comment_text.value;
if (comment.replace(/^\s+|\s+$/g, '').length < 4) {
    alert('Please enter a comment of at least 4 characters in length!');
    return false;
}

return true;
}
