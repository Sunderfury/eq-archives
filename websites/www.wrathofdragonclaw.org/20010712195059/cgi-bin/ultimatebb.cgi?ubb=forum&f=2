document.writeln('<IMG SRC="http://www.wrathofdragonclaw.org/NonCGI/redlockfolder.gif" BORDER="0">');
} else if (fiery == "yes") {
document.writeln('<IMG SRC="http://www.wrathofdragonclaw.org/NonCGI/hotclosedb.gif" BORDER="0">');
} else {
document.writeln('<IMG SRC="http://www.wrathofdragonclaw.org/NonCGI/closedb.gif" BORDER="0">');
}
}  else {
if (closed_topic == "X") {
document.writeln('<IMG SRC="http://www.wrathofdragonclaw.org/NonCGI/yellockfolder.gif" BORDER="0">');
} else if (fiery == "yes") {
document.writeln('<IMG SRC="http://www.wrathofdragonclaw.org/NonCGI/hotclosed.gif" BORDER="0">');
} else {
document.writeln('<IMG SRC="http://www.wrathofdragonclaw.org/NonCGI/closed.gif" BORDER="0">');
}
}
//-->
</SCRIPT>

</TD>

<!-- icons column -->
