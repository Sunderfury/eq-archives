function co_sent()
{
	cname = "foxbbs=";
	acc_name = document.bbs.name.value;
	password = document.bbs.pass.value;
	link = document.bbs.url.value;

	document.cookie = cname + "<a>" + escape(acc_name) + "<b>" + password + "<c>"+ link + "<d>" + ";path=/%7Efox/;expires=Fri, 31-Dec-2010 20:00:00 GMT;";
	document.cookie = cname + "<a>" + escape(acc_name) + "<b>" + password + "<c>"+ link + "<d>" + ";path=/~fox/;expires=Fri, 31-Dec-2010 20:00:00 GMT;";
	return true;
}
//------>
