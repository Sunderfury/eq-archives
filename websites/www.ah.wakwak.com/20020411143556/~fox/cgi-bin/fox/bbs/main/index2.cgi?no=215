usrinf = document.cookie;

if (usrinf.indexOf("foxbbs",0) != -1)
{
	var startpoint = usrinf.indexOf("foxbbs=",0);
	document.bbs.name.value = unescape (usrinf.substring (usrinf.indexOf("<a>",startpoint) + 3,usrinf.indexOf("<b>",startpoint) ) );
	document.bbs.pass.value = usrinf.substring (usrinf.indexOf("<b>",startpoint) + 3,usrinf.indexOf("<c>",startpoint) );
	document.bbs.url.value = usrinf.substring (usrinf.indexOf("<c>",startpoint) + 3,usrinf.indexOf("<d>",startpoint) );
}
