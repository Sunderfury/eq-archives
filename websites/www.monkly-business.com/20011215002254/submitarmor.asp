        <p><br>
          <br>
          <span class="MB_Heading"> Submit Armor</span></p>
        <ul>
          <form name="form1" method="post" action="checksubmitarmor.asp">
            <p>* = Required Field</p>
            <p>*Slot: 
              <select name="SlotId">
                <option value="0" selected>Select One</option>
                
                <option value="7" >Arm</option>
                
                <option value="11" >Back</option>
                
                <option value="6" >Bracer</option>
                
                <option value="10" >Chest</option>
                
                <option value="1" >Ear</option>
                
                <option value="3" >Face</option>
                
                <option value="13" >Feet</option>
                
                <option value="5" >Finger</option>
                
                <option value="8" >Hand</option>
                
                <option value="4" >Head</option>
                
                <option value="12" >Legs</option>
                
                <option value="2" >Neck</option>
                
                <option value="9" >Shoulder</option>
                
                <option value="14" >Waist</option>
                
              </select>
            </p>
            <p>*Name: 
              <input type="text" name="Name" maxlength="50" size="50">
            </p>
            <p>*Magic? 
              <select name="Magic">
                <option value="x" selected>Select One</option>
                <option value="Yes">Yes</option>
                <option value="No">No</option>
              </select>
            </p>
            <p>*Lore? 
              <select name="Lore">
                <option value="x" selected>Select One</option>
                <option value="Yes">Yes</option>
                <option value="No">No</option>
              </select>
            </p>
            <p>*No Drop? 
              <select name="NoDrop">
                <option value="x" selected>Select One</option>
                <option value="Yes">Yes</option>
                <option value="No">No</option>
              </select>
            </p>
            <p>AC: 
              <input type="text" name="AC" maxlength="3" size="3">
              &nbsp;&nbsp;&nbsp; Hit Points: 
              <input type="text" name="HP" size="3" maxlength="3">
              &nbsp;&nbsp;&nbsp;Mana: 
              <input type="text" name="Mana" size="3" maxlength="3">
            </p>
            <p>Strength: 
              <input type="text" name="Strength" size="3" maxlength="3">
              &nbsp;&nbsp;&nbsp;Stamina: 
              <input type="text" name="Stamina" size="3" maxlength="3">
              &nbsp;&nbsp;&nbsp;Agility: 
              <input type="text" name="Agility" size="3" maxlength="3">
              &nbsp;&nbsp;&nbsp;Dexterity: 
              <input type="text" name="Dexterity" size="3" maxlength="3">
            </p>
            <p>Intelligence: 
              <input type="text" name="Intelligence" size="3" maxlength="3">
              &nbsp;&nbsp;&nbsp;Wisdom: 
              <input type="text" name="Wisdom" size="3" maxlength="3">
              &nbsp;&nbsp;&nbsp;Charisma: 
              <input type="text" name="Charisma" size="3" maxlength="3">
            </p>
            <p>Sv Magic: 
              <input type="text" name="Sv Magic" size="3" maxlength="3">
              &nbsp;&nbsp;&nbsp;Sv Fire: 
              <input type="text" name="Sv Fire" size="3" maxlength="3">
              &nbsp;&nbsp;&nbsp;Sv Cold: 
              <input type="text" name="Sv Cold" size="3" maxlength="3">
            </p>
            <p>Sv Disease: 
              <input type="text" name="Sv Disease" size="3" maxlength="3">
              &nbsp;&nbsp;&nbsp;Sv Poison: 
              <input type="text" name="Sv Poison" size="3" maxlength="3">
            </p>
            <p>*Size: 
              <select name="SizeEQ">
                <option value="x" selected>Select One</option>
                <option value="Tiny">Tiny</option>
                <option value="Small">Small</option>
                <option value="Medium">Medium</option>
                <option value="Large">Large</option>
                <option value="X-Large">X-Large</option>
              </select>
            </p>
            <p>*Race: 
              <select name="Race">
                <option value="0" selected>Select One</option>
                <option value="1">Human</option>
                <option value="3">Iksar</option>
                <option value="2">Both</option>
              </select>
            </p>
            <p>*Weight: 
              <input type="text" name="Weight" size="4" maxlength="4">
            </p>
            <p>*Effect Type: 
              <select name="EffectType">
                <option value="x" selected>Select One</option>
                <option value="None">None</option>
                <option value="Perm">Perm</option>
                <option value="Click">Click</option>
                <option value="Proc">Proc</option>
              </select>
            </p>
            <p>Effect: 
              <input type="text" name="Effect" size="50" maxlength="50">
            </p>
            <p>Charges: 
              <input type="text" name="Charges" size="2" maxlength="2">
              (enter 99 for unlimited charges)</p>
            <p>*Light Source: 
              <select name="LightSource">
                <option value="x" selected>Select One</option>
                <option value="No">No</option>
                <option value="Lesser">Lesser</option>
                <option value="Greater">Greater</option>
              </select>
            </p>
            <p align="center"> 
              <input type="submit" name="Submit" value="Submit">
            </p>
          </form>
        </ul>
      </ul>
