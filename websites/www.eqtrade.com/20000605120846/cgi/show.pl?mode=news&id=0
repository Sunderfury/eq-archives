		document.write("<td width=*><img src=center_bottom.jpg height=26 width=1420></td>");
	}
	if ((screen.width!=640) && (screen.width!=1024) && (screen.width!=1280) && (screen.width!=1600) && (screen.width!=1152) && (screen.width!=800)){
		document.write("<td width=*><img src=center_bottom.jpg height=26 width=100%></td>");
	}
</script>
<td width=8><img src=center_right.jpg></td>
</tr>
</table>
</td></tr>
</table>
</td>
<td valign=bottom>
<table border=0 cellpadding=0 cellspacing=0 width=100%>
<tr><td><img src=right_bottom.jpg></td></tr>
</table>
</td>
