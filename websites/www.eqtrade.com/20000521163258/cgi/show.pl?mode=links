<script>
  change = new Array(5);
  normal = new Array(5);
  change[0] = new Image(83, 13); change[0].src = "menu_news2.jpg";
  normal[0] = new Image(83, 13); normal[0].src = "menu_news.jpg";
  change[1] = new Image(83, 13); change[1].src = "menu_board2.jpg";
  normal[1] = new Image(83, 13); normal[1].src = "menu_board.jpg";
  change[2] = new Image(83, 13); change[2].src = "menu_link2.jpg";
  normal[2] = new Image(83, 13); normal[2].src = "menu_link.jpg";
  change[3] = new Image(83, 13); change[3].src = "menu_file2.jpg";
  normal[3] = new Image(83, 13); normal[3].src = "menu_file.jpg";
  change[4] = new Image(83, 13); change[4].src = "menu_player2.jpg";
  normal[4] = new Image(83, 13); normal[4].src = "menu_player.jpg";
  change[5] = new Image(83, 13); change[5].src = "menu_mail2.jpg";
  normal[5] = new Image(83, 13); normal[5].src = "menu_mail.jpg";
function change_image(num) {
 if (document.images[num+1].src==normal[num].src) {
   document.images[num+1].src = change[num].src;
 }
 else {
   document.images[num+1].src = normal[num].src;
 }
}
</script>
