<script language="JavaScript">

function delChar(lngCharID, lngPersonId, strChar)
{
	if (confirm("Are you sure you want to delete " + strChar) == true)
	{
		parent.location="uu_char_del.asp?charid=" + lngCharID+ "&pid=" + lngPersonId
	}
}

function delGuild(lngPersonId)
{
	if (confirm("Are you sure you want to remove this person from the Guild?") == true)
	{
		parent.location="uu_del_guild.asp?pid=" + lngPersonId
	}
}

function saveChar(charForm)
{
	if (charForm.firstname.value == '')
	{
		alert('Firstname must have a value');
		charForm.firstname.focus();
	}
	else if (charForm.level.value == '')
	{
		alert('Level must have a value');
		charForm.level.focus();
	}
	else
	{
		charForm.submit();
	}
}

function savePerson(persForm)
{
	if (persForm.firstname.value == '')
	{
		alert('Firstname must have a value');
		persForm.firstname.focus();
	}
	else if (persForm.surname.value == '')
	{
		alert('Surname must have a value');
		persForm.surname.focus();
	}
	else if (persForm.nickname.value == '')
	{
		alert('Nickname must have a value');
		persForm.nickname.focus();
	}
	else if (persForm.email.value == '')
	{
		alert('Email Address must have a value');
		persForm.email.focus();
	}
	else if (persForm.sex.value == '')
	{
		alert('Sex must have a value');
		persForm.sex.focus();
	}
	else if (persForm.rlloc.value == '')
	{
		alert('Real Life Location must have a value');
		persForm.rlloc.focus();
	}
	else
	{
		persForm.submit();
	}
}

</script>

