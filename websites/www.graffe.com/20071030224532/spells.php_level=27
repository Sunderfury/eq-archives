var lastTab = new Array();
function toggleView(sID, szDivID, iState) // 1 visible, 0 hidden, 2 toggle
{
  	if (document.getElementById){
		if(sID != 'notab' && window.lastTab[sID]) {
			document.getElementById( lastTab[sID] ).style.display = "none";
		}
		lastTab[sID] = szDivID;
