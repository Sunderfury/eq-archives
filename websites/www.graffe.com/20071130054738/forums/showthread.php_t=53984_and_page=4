                                  <!-- WoW Style Avatars --><br><table cellspacing ='0' cellpadding ='0' border ='0'><tr><td colspan ='3'><img src ='/forums/images/wowborder/portrait-top.gif' width ='82' height ='8'></td></tr><tr><td><img src ='/forums/images/wowborder/portrait-left.gif' width ='8' height ='64'></td><td background='image.php?u=3439&amp;dateline=1159357652' style='background-repeat: no-repeat; background-position: center center;'><table cellspacing ='0' cellpadding ='0' border ='0'><tr><td background ='/forums/images/wowborder/level-circle.gif'><div style ='height:64px; width:64px;'><div style ='position: relative; top:52px; left: 44px; width:28px; text-align:center; color: #FFD823;'><b><small>70</small></b></div></td></tr></table></td><td><img src='/forums/images/wowborder/portrait-right.gif' Border='0' width =' 10' height =' 64'></td></tr><tr><td colspan =' 3'><img src ='/forums/images/wowborder/portrait-bot.gif' width =' 82' height =' 11'></td></tr></table><!-- / WoW Style Avatars -->		
				
				
			


