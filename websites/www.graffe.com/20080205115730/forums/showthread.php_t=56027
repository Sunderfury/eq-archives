	<div id="edit1289396" style="padding:0px 0px 6px 0px">
	<!-- this is not the last post shown on the page -->	


<table id="post1289396" class="tborder" cellpadding="6" cellspacing="0" border="0" width="100%" align="center">
<tr>
	
		<td class="thead" style="font-weight:normal" >		
			<!-- status icon and date -->
			<a name="post1289396"><img class="inlineimg" src="images/legacy_images/statusicon/post_old.gif" alt="Old" border="0" /></a>
