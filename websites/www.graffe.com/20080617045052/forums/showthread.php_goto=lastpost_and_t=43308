	<td class="alt1" id="td_post_984357" style="border-right: 1px solid #D1D1E1">
	
		
		
			<!-- icon and title -->
			<div class="smallfont">
				
				<strong>TSS Spells</strong>
			</div>
			<hr size="1" style="color:#D1D1E1; background-color:#D1D1E1" />
