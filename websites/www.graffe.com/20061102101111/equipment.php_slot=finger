var bgdir = "images/image_backgrounds/"

images = new Array;
images[0] = "001.jpg";
images[1] = "002.jpg";
images[2] = "003.jpg";
images[3] = "004.jpg";
images[4] = "005.jpg";
images[5] = "006.jpg";
images[6] = "007.jpg";
images[7] = "008.jpg";
images[8] = "009.jpg";
images[9] = "010.jpg";
