<SCRIPT LANGUAGE="javascript"><!--
	//by merloc@graffe.com v1.2
	sn = new Array("","","iname","data[item]","searchtext","","name","","name","name","searchtext");
	sa = new Array("www.graffe.com/eqsearch.html",
	"", "everquest.allakhazam.com/db/searchdb.html",
	"www.eqtraders.com/search/search.php",
	"lucy.allakhazam.com/itemlist.html",
	"", "eqbeastiary.allakhazam.com/search.shtml",
	"", "everquest.allakhazam.com/db/spelllist.html",
	"www.graffe.com/spells.php",
	"lucy.allakhazam.com/spelllist.html");

	function picksite(si) {
		d = document.eqs;
		if(si == 3 && !d.elements[2]) {
			var m = document.createElement('INPUT');
			m.type = "hidden";
			m.name = "min";
			m.value = "0";
			d.appendChild(m);
		}
		else if (d.elements[2]){
			d.removeChild(d.elements[2]);
		}
		d.method = (si == 3) ? "post" : "get";
		d.elements[1].disabled = d.elements[0].disabled = sa[si] ? false : true;
		d.elements[0].className = sa[si] ? "y" : "x";
		d.action = "http://" + sa[si];
		d.elements[0].name = sn[si];
		return false;
	}
//  --></SCRIPT>
<form style="display:inline;"><select onChange="picksite(this.selectedIndex);">
