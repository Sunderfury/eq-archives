&nbsp; &nbsp; &nbsp; &nbsp; {<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; allT[i].style.display=&quot;none&quot;;<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; //Add ignored user to list of ignored users<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; plonk[RegExp.$1] = RegExp.$1;<br />
&nbsp; &nbsp; &nbsp; &nbsp; }<br />
