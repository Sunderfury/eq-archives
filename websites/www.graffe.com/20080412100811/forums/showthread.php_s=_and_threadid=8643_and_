<!-- Profiles -->
<table width=100%><tr><td align=right width=25%><div class='smallfont'><b>Game</b>:</div></td><td width=75%><div class='smallfont'>World of Warcraft</div></td></tr><tr><td align=right width=25%><div class='smallfont'><b>Name</b>:</div></td><td width=75%><div class='smallfont'>Remmie</div></td></tr><tr><td align=left colspan=2><div id='postprofile_167919'><span class='smallfont' style='cursor: pointer;'><b>WoW Profile</b></span><script type='text/javascript'> vbmenu_register('postprofile_167919', false); </script></div></td></tr></table> 
