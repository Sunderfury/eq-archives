<tr valign="top">
	<td class="alt2" width="175">
	
	
			<div id="postmenu_919618">
				
				
				<a class="bigusername" href="member.php?u=5495">Tangurena</a>
				<script type="text/javascript"> vbmenu_register("postmenu_919618", true); </script>
				
			</div>
			
			<div class="smallfont">Forum Arcanist</div>
			
			
		
			
			
			
						
			
			<div class="smallfont">
				&nbsp;<br />
				
				
				
				&nbsp;<br />
				<div>   </div>
