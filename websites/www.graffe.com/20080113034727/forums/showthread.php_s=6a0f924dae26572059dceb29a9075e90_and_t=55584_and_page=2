<!-- Profiles -->
<table width=100%><tr><td align=right width=25%><div class='smallfont'><b>Game</b>:</div></td><td width=75%><div class='smallfont'>Vanguard</div></td></tr><tr><td align=left colspan=2><div id='postprofile_1280050'><span class='smallfont' style='cursor: pointer;'><b>Vanguard Profile</b></span><script type='text/javascript'> vbmenu_register('postprofile_1280050', false); </script></div></td></tr></table> 
