<!-- Profiles -->
<table width=100%><tr><td align=right width=25%><div class='smallfont'><b>Game</b>:</div></td><td width=75%><div class='smallfont'>World of Warcraft</div></td></tr><tr><td align=right width=25%><div class='smallfont'><b>Name</b>:</div></td><td width=75%><div class='smallfont'>Voca</div></td></tr><tr><td align=left colspan=2><div id='postprofile_1249196'><span class='smallfont' style='cursor: pointer;'><b>WoW Profile</b></span><script type='text/javascript'> vbmenu_register('postprofile_1249196', false); </script></div></td></tr></table> 
<!-- / Profiles -->



				&nbsp;<br />
				<div>  <a href="#" onclick="imwindow('msn', '218', 400, 200); return false;"><img src="images/legacy_images/misc/im_msn.gif" alt="Send a message via MSN to Voca" border="0" /></a> </div>
			</div>				
	
	
	
