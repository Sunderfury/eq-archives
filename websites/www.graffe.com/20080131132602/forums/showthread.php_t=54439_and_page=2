	<div id="edit1257890" style="padding:0px 0px 6px 0px">
	<!-- this is not the last post shown on the page -->	


<table id="post1257890" class="tborder" cellpadding="6" cellspacing="0" border="0" width="100%" align="center">
<tr>
	
		<td class="thead" style="font-weight:normal" >		
			<!-- status icon and date -->
