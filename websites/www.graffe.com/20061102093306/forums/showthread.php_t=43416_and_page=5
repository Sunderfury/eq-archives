<tr valign="top">
	<td class="alt2" width="175">
	
	
			<div id="postmenu_1013161">
				
				
				<a class="bigusername" href="member.php?u=2">Gemdiver</a>
				<script type="text/javascript"> vbmenu_register("postmenu_1013161", true); </script>
				
			</div>
			
			<div class="smallfont">Caster's Realm</div>
			
			
		
			
			
				
				
					<!-- Standard Avatars -->
					
					<div class="smallfont">
						&nbsp;<br /><a href="member.php?u=2"><img src="image.php?u=2&amp;dateline=1058237896" alt="Gemdiver's Avatar" border="0" /></a>
