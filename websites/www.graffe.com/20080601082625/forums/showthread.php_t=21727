<tr valign="top">
	<td class="alt2" width="175" style="border: 1px solid #D1D1E1; border-top: 0px; border-bottom: 0px">

			<div id="postmenu_476640">
				
				<a class="bigusername" href="member.php?u=2439">Caemien</a>
				<script type="text/javascript"> vbmenu_register("postmenu_476640", true); </script>
				
			</div>

			<div class="smallfont">Forum Wizard</div>
			
			

			

			<div class="smallfont">
				&nbsp;<br />
				<div>Join Date: Sep 2003</div>
				
				
				<div>
					Posts: 189
				</div>
				
				
				
				
				<div>    </div>
			</div>

