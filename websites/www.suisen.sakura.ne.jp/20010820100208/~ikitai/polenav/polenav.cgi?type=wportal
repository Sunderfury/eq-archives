function sinki() {
  if (document.F1.name.value == "") {
    alert("名前が入力されていません。");
    document.F1.name.focus();
    return(false);
  }
  if (document.F1.x.value == "") {
    alert("Ｘ座標が入力されていません。");
    document.F1.x.focus();
    return(false);
  }
  if (document.F1.y.value == "") {
    alert("Ｙ座標が入力されていません。");
    document.F1.y.focus();
    return(false);
  }
  if (document.F1.pwd.value == "") {
    alert("削除キーが入力されていません。");
    document.F1.pwd.focus();
    return(false);
  }
  return(true);
}
// -->
</SCRIPT>

<FORM NAME=F1 method=POST action=polenav.cgi onSubmit="return sinki()">
<INPUT type=hidden name=type value=wportal>
<INPUT type=hidden name=mode value=new>
<INPUT type=hidden name=pname value="">
データーの追加・削除
