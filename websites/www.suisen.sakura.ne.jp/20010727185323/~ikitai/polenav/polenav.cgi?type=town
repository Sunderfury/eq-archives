function sinki() {
  if (document.F1.name.value == "") {
    alert("名前が入力されていません。");
    document.F1.name.focus();
    return(false);
  }
  if (document.F1.x.value == "") {
    alert("Ｘ座標が入力されていません。");
    document.F1.x.focus();
    return(false);
  }
  if (document.F1.y.value == "") {
    alert("Ｙ座標が入力されていません。");
    document.F1.y.focus();
    return(false);
  }
  if (document.F1.pwd.value == "") {
    alert("削除キーが入力されていません。");
    document.F1.pwd.focus();
    return(false);
  }
  return(true);
}
// -->
</SCRIPT>

