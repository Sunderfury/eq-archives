function check() {
  if (document.F1.name.value == "") {
    alert("名前が入力されていません。");
    document.F1.name.focus();
    return(false);
  }
  if (document.F1.pwd.value == "") {
    alert("パスワードが入力されていません。");
    document.F1.pwd.focus();
    return(false);
  }
  if (document.F1.pname.value == "") {
    alert("キャラ名が入力されていません。");
    document.F1.pname.focus();
    return(false);
  }
  if (document.F1.syokugyo.value == "") {
    alert("職業が入力されていません。");
    document.F1.syokugyo.focus();
    return(false);
  }
  s = document.F1.str.value;
  d = document.F1.dex.value;
  i = document.F1.elements["int"].value;
  status = eval(s) + eval(d) + eval(i);
  if (status > 226) {
    alert("ステータスの合計が225を越えてます。"+status);
    document.F1.comment.focus();
    return(false);
  }
  if (document.F1.comment.value == "") {
    alert("コメントが入力されていません。");
    document.F1.comment.focus();
    return(false);
  }
  return(true);
}
// -->
