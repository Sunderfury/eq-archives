    var midR = document.getElementById('midR');
    var targ = parseInt(document.getElementById('midL').offsetHeight);
    midR.style.height = targ + "px";
  }
resizemidR();
function waitandresizemidR(){
  setTimeout("resizemidR()",10);
}
