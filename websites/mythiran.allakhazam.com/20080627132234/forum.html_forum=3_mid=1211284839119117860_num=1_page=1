<script type="text/javascript">
  function resizemidR(){
    var midR = document.getElementById('midR');
    var targ = parseInt(document.getElementById('midL').offsetHeight);
    midR.style.height = targ + "px";
  }
resizemidR();
function waitandresizemidR(){
  setTimeout("resizemidR()",10);
}
 addEvent(window,'mouseup',waitandresizemidR);

//document.getElementById('mainP').style.height = targ +"px";
</script>
<!--[if lte IE 6]>
<script type="text/javascript">
sfHover = function() {
  var sfEls = document.getElementById("topMenu").getElementsByTagName("li");
  for (var i=0; i<sfEls.length; i++) {
    sfEls[i].onmouseover=function() {
      this.className+=" sfhover";
      shimme = this.lastChild;
    }
    sfEls[i].onmouseout=function() {
      this.className=this.className.replace(new RegExp("sfhover"), "");
    }
  }
}
if (window.attachEvent) window.attachEvent("onload", sfHover);
</script>
